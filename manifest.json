{
    "manifest_version": 3,
    "name": "Timer Extension",
    "version": "1.0",
    "description": "A simple timer extension.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/react_chrome.png",
        "48": "images/react_chrome.png",
        "128": "images/react_chrome.png"
      }
    },
    "icons": {
      "16": "images/react_chrome.png",
      "48": "images/react_chrome.png",
      "128": "images/react_chrome.png"
    },
    "background": {
      "service_worker": "popup.js"
    }
  }
  